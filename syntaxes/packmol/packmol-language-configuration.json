{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(structure|end structure)\\s*$",
    "decreaseIndentPattern": "^\\s*(end structure)\\s*$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*structure\\s+",
      "end": "^\\s*end\\s+structure\\s*$"
    }
  }
}
