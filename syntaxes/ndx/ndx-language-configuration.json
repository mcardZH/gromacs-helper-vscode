{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "offSide": true,
    "markers": {
      "start": "^\\s*\\[\\s*[^\\]]+\\s*\\]\\s*$",
      "end": "^\\s*(?=\\[\\s*[^\\]]+\\s*\\])|^\\s*$"
    }
  },
  "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*"
}
